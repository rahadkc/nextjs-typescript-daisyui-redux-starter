{
  "name": "nextjs-typescript-redux-starter",
  "version": "1.0.8",
  "bin": "./bin/cli.js",
  "scripts": {
    "dev": "next dev -p 3050",
    "build": "next build",
    "start": "next start -p 3050",
    "lint": "next lint",
    "test": "jest --watchAll",
    "test:verbose": "jest --watchAll --verbose",
    "test:exit": "jest --forceExit",
    "test:clear": "jest --clearCache",
    "format": "prettier --write .",
    "find:unused": "next-unused",
    "setup:husky": "npm pkg set scripts.prepare=\"husky install\" && npm run prepare && npx husky add .husky/pre-commit \"npm run lint\" && npx husky add .husky/pre-commit \"npm run test:exit\" && git add .husky/pre-commit",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3050 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3050 \"cypress run --e2e\"",
    "e2e:component": "cypress open --component",
    "e2e:component:headless": "cypress run --component"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^1.9.7",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "mongoose": "^7.6.1",
    "next": "^13.4.19",
    "react": "^18",
    "react-big-calendar": "^1.8.4",
    "react-dom": "^18",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "rrule": "^2.7.2",
    "swr": "^2.2.4",
    "usehooks-ts": "^2.9.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@next/bundle-analyzer": "^13.5.4",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-big-calendar": "^1.8.0",
    "@types/react-dom": "^18",
    "@types/react-redux": "^7.1.27",
    "@types/redux-logger": "^3.0.10",
    "autoprefixer": "^10",
    "cypress": "^13.5.0",
    "daisyui": "^3.9.2",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-unused": "^0.0.6",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "redux-logger": "^3.0.6",
    "start-server-and-test": "^2.0.2",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahadkc/nextjs-typescript-redux-jest-cypress-starter"
  },
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  }
}
